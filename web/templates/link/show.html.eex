<h2>Show link</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @link.name %>
  </li>

  <li>
    <strong>Url:</strong>
    <%= @link.url %>
  </li>

  <li>
    <strong>Query Url:</strong>
    <%= @link.query_url %>
  </li>

</ul>

<%= link "Edit", to: link_path(@conn, :edit, @link) %>
<%= link "Back", to: link_path(@conn, :index) %>
